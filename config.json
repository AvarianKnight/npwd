{
  "PhoneAsItem": {
    "enabled": true,
    "exportResource": "pma-lib",
    "exportFunction": "canUsePhone"
  },
  "general": {
    "useResourceIntegration": true,
    "toggleKey": "U",
    "toggleCommand": "phone",
    "defaultLanguage": "en"
  },
  "profanityFilter": {
    "enabled": true,
    "badWords": ["esx"]
  },
  "database": {
    "useIdentifierPrefix": false,
    "playerTable": "users",
    "identifierColumn": "id",
    "identifierType": "license",
    "profileQueries": true,
    "phoneNumberColumn": "phone_number"
  },
  "debug": {
    "level": "error",
    "enabled": true,
    "sentryEnabled": true
  }
}
